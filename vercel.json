{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "public, max-age=31536000" },
      "continue": true
    },
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000" },
      "continue": true
    },
    {
      "src": "/(.+\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff2|ttf|eot|json|xml|txt))$",
      "headers": { "cache-control": "public, max-age=31536000" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
